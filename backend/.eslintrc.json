{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "space-before-function-paren": "off",
    "prettier/prettier": ["error"]
  }
}
